{
  "name": "fastify-jsdoc-ts",
  "version": "1.0.0",
  "description": "A Schema first NodeJS Fastify JSDoc Typescript server",
  "main": "src/index.mjs",
  "type": "module",
  "imports": {
    "#src/*.mjs": "./src/*.mjs"
  },
  "scripts": {
    "test": "node --test --test-reporter spec",
    "lint": "./node_modules/.bin/eslint --fix",
    "typecheck": "./node_modules/.bin/tsc --noEmit",
    "check": "npm run lint && npm run typecheck && npm run test",
    "dev": "npm run typecheck && ./node_modules/.bin/nodemon",
    "start": "npm run check && node ./src/index.mjs",
    "db:migrate:generate": "pnpm exec drizzle-kit generate --config ./drizzle.config.ts",
    "db:migrate:run": "node ./src/db/migrate.mjs",
    "db:migrate": "npm run db:migrate:generate && db:migrate:run",
    "db:studio:launch": "pnpm exec drizzle-kit studio --port 6139 --host 0.0.0.0",
    "prepare": "husky"
  },
  "author": "michaelolof",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "9.5.0",
    "@stylistic/eslint-plugin": "^2.2.1",
    "@types/eslint__js": "8.42.3",
    "@types/node": "^20.11.30",
    "drizzle-kit": "^0.22.7",
    "eslint": "9.5.0",
    "globals": "15.6.0",
    "husky": "^9.0.11",
    "nodemon": "^3.1.3",
    "typescript": "^5.4.3",
    "typescript-eslint": "7.13.1"
  },
  "dependencies": {
    "@fastify/autoload": "^5.8.0",
    "@fastify/cors": "^9.0.1",
    "@fastify/error": "^3.4.1",
    "@fastify/swagger": "^8.14.0",
    "@fastify/swagger-ui": "^3.0.0",
    "@fastify/type-provider-typebox": "^4.0.0",
    "@sinclair/typebox": "^0.32.33",
    "close-with-grace": "^1.3.0",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.31.2",
    "fastify": "^4.26.2",
    "pino": "^9.2.0",
    "pino-pretty": "^11.2.1",
    "postgres": "^3.4.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelolof/fastify-jsdoc-dts.git"
  },
  "engine": {
    "node": "^20.x.x",
    "npm": "^10.x.x"
  }
}